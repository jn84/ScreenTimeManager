@model ScreenTimeManager.Models.TotalScreenTimeChanged

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title" id="myModalLabel">Time Added Confirmation</h4>
</div>

<div class="modal-body">
    <h4>RuleBase</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Id)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RecordAddedDateTime)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.RuleUsedId)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SecondsAdded)
        </dd>
    </dl>
</div>
<div class="modal-footer">
    <button class="btn" data-dismiss="modal">Ok</button>
</div>
